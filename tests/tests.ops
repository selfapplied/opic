def test { name, input, expected, actual }
def assertion { condition, message }

voice test.loss.functions / {prediction + target -> loss}
voice test.optimization / {weight + loss + learning_rate -> updated.weight}
voice test.synthesis / {quantum + archetype -> hybrid}

voice assert.mse / {prediction + target -> loss == (prediction - target)^2}
voice assert.optimization / {weight + gradient -> weight - gradient}
voice assert.synthesis / {quantum + archetype -> (quantum, archetype)}

target test.expectations / "test.spec"
voice main / {test.name -> test.expectations}
